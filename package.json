{
  "name": "yesno",
  "version": "0.0.0",
  "scripts": {
    "eslint": "eslint app boot config",
    "start": "grown up",
    "dev": "tarima -w --server --port 3001",
    "dev:reload": "tarima -w --server --proxy localhost:8080 --port 3001 --reload app/reload",
    "dev:grown": "tarima -w --server --proxy localhost:8080 --port 3001 -- grown up --port 8080",
    "heroku-postbuild": "tarima -fq"
  },
  "tarima": {
    "src": "_src/**",
    "dest": ".",
    "public": "public",
    "watch": [
      ".env",
      "package.json"
    ],
    "filter": [
      "!_*",
      "!**/_*",
      "!**/_*/**"
    ],
    "bundle": [
      "**/index.js"
    ],
    "rename": [
      "**:{fullpath/1}",
      "**/index.(css|js):{basedir}.{ext}"
    ],
    "ignoreFiles": [
      ".gitignore"
    ],
    "devPlugins": [
      "tarima-lr"
    ],
    "plugins": [
      "talavera"
    ],
    "pluginOptions": {
      "talavera": {
        "dest": "images"
      }
    },
    "bundleOptions": {
      "bundleCache": true,
      "entryCache": true,
      "extensions": {
        "js": "es6",
        "css": "less"
      },
      "less": {
        "plugins": [
          "less-plugin-autoprefix"
        ]
      }
    }
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "chokidar": "^1.6.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.1",
    "cors": "^2.8.3",
    "csurf": "^1.9.0",
    "formidable": "^1.1.1",
    "grown": "pateketrueke/grown",
    "json-schema-sequelizer": "^0.0.2",
    "morgan": "^1.8.1",
    "route-mappings": "^0.3.2",
    "serve-static": "^1.12.1",
    "sqlite3": "^3.1.8",
    "tarima-lr": "^0.0.12"
  },
  "optionalDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0"
  },
  "devDependencies": {
    "buble": "^0.15.2",
    "less": "^2.7.2",
    "less-plugin-autoprefix": "^1.5.1",
    "pug": "^2.0.0-beta.12",
    "rollup": "^0.41.6",
    "talavera": "^0.2.2",
    "tarima-cli": "gextech/tarima-cli"
  }
}
